{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "title": "register Validation",
    "required": ["email_id","password","phone_num","cramp_cycle","menstrual_cycle","life_cycle","preg_cycle","bleed_cycle","id_number"],
    "properties": {
      "email_id":{
        "type": "string",
        "title": "emailid schema",
        "pattern": "^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:.[a-zA-Z0-9-]+)*$"
      },
      "password":{
          "type": "string",
          "minLength": 8,
          "title": "password schema"
      },
      "phone_num":{
        "type": "string",
        "title": "phone number schema",
        "pattern": "[0-9]+$"
      },
      "cramp_cycle":{
        "type" : "number",
        "title": "cramp cycle schema"
      },
      "life_cycle":{
        "type" : "number",
        "title": "life cycle schema"
      },
      "preg_cycle":{
        "type" : "number",
        "title": "preg cycle schema"
      },
      "menstrual_cycle":{
        "type" : "number",
        "title": "menstrual cycle schema"
      },
      "bleed_cycle":{
        "type" : "number",
        "title": "bleed cycle schecma"
      },
      "id_number":{
        "type" : "number",
        "title": "id number schema"
      }
    }
  }